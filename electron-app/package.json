{
  "name": "svg-app-builder",
  "version": "0.1.0",
  "description": "Local SVG app builder with Ollama & Qdrant",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "react-scripts build",
    "dist": "electron-builder --win nsis"
  },
  "author": "Your Name",
  "dependencies": {
    "axios": "^1.4.0",
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-konva": "^18.0.0-rc.1",
    "konva": "^9.2.1"
  },
  "devDependencies": {
    "electron": "^26.0.0",
    "electron-builder": "^24.6.0"
  },
  "build": {
    "appId": "com.example.svgappbuilder",
    "win": {
      "target": "nsis"
    },
    "extraResources": [
      {
        "from": "resources/",
        "to": "resources",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
